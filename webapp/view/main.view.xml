<mvc:View controllerName="evaluatorweb.controller.main"
    xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
	xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:tnt="sap.tnt"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    height="100%"
     width="100%">
   
    
    <App id="Main"> 
    <tnt:ToolPage id="toolPage">
        <tnt:header>
            <tnt:ToolHeader id="_IDGenToolHeader1">

                <Toolbar id="_IDGenToolbar1">
                    <Image id="_IDGenImage1" src="/images/logo-isc.png" width="30px" height="30px"/>
                </Toolbar>
                <ToolbarSpacer id="_IDGenToolbarSpacer1" width="20px" />
                <Button id="_IDGenButton1" text="Acerca de" type="Transparent">
                    <layoutData>
                        <OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData2" priority="Low" />
                    </layoutData>
                </Button>
                <Button id="_IDGenButton2" text="Help people" type="Transparent">
                    <layoutData>
                        <OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData3" priority="Low" />
                    </layoutData>
                </Button>
                <tnt:ToolHeaderUtilitySeparator id="_IDGenToolHeaderUtilitySeparator1" />
                <ToolbarSpacer id="_IDGenToolbarSpacer2">
                    <layoutData>
                        <OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData10" priority="NeverOverflow" minWidth="20px" />
                    </layoutData>
                </ToolbarSpacer>
                <Button id="_IDGenButton9" text="Harby Mina" press="handleUserNamePress" ariaHasPopup="Menu">
                    <layoutData>
                        <OverflowToolbarLayoutData id="_IDGenOverflowToolbarLayoutData11" priority="NeverOverflow" />
                    </layoutData>
                </Button>
            </tnt:ToolHeader>
        </tnt:header>


        <tnt:sideContent>
            <tnt:SideNavigation id="_IDGenSideNavigation1" expanded="{data>/sideNavExpanded}" selectedKey="{data>/selectedKey}" itemSelect=".onItemSelect" class="miClasePersonalizada">
                <tnt:NavigationList id="_IDGenNavigationList1" items="{path: 'data>/navigation'}">
                    <tnt:NavigationListItem id="_IDGenNavigationListItem1" text="{data>title}" icon="{data>icon}" enabled="{data>enabled}" expanded="{data>expanded}" items="{data>items}" key="{data>key}">
                        <tnt:NavigationListItem id="_IDGenNavigationListItem2" text="{data>title}" key="{data>key}" enabled="{data>enabled}" />
                    </tnt:NavigationListItem>
                </tnt:NavigationList>
                <tnt:fixedItem>
                    <tnt:NavigationList id="_IDGenNavigationList2" items="{path: 'data>/fixedNavigation'}">
                        <tnt:NavigationListItem id="_IDGenNavigationListItem3" text="{data>title}" icon="{data>icon}" />
                    </tnt:NavigationList>
                </tnt:fixedItem>
            </tnt:SideNavigation>
            
        </tnt:sideContent>


        <tnt:mainContents>
            <NavContainer id="pageContainer" initialPage="Principal">
                <pages>
                    <ScrollContainer id="Principal" horizontal="false" vertical="true" height="100%">
                        <mvc:XMLView id="_IDGenXMLView1" viewName="evaluatorweb.view.Principal"/>
                    </ScrollContainer>

                    <ScrollContainer id="Temporadas" horizontal="false" vertical="true" height="100%">
                        <mvc:XMLView id="_IDGenXMLView2" viewName="evaluatorweb.view.Temporadas"/>
                    </ScrollContainer> 

                    <ScrollContainer id="Usuarios" horizontal="false" vertical="true" height="100%">
                        <mvc:XMLView id="_IDGenXMLView3" viewName="evaluatorweb.view.Usuarios"/>
                    </ScrollContainer> 

                    <ScrollContainer id="Evaluaciones" horizontal="false" vertical="true" height="100%">
                        <mvc:XMLView id="_IDGenXMLView4" viewName="evaluatorweb.view.Evaluaciones"/>
                    </ScrollContainer> 

                    <ScrollContainer id="Empleados" horizontal="false" vertical="true" height="100%">
                        <mvc:XMLView id="_IDGenXMLView5" viewName="evaluatorweb.view.Empleados"/>
                    </ScrollContainer> 

                    
                    <ScrollContainer id="Nueva_Evaluacion" horizontal="false" vertical="true" height="100%">
                        <mvc:XMLView id="_IDGenXMLView6" viewName="evaluatorweb.view.Nueva_Evaluacion"/>
                    </ScrollContainer> 


                </pages>
            </NavContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</App>
</mvc:View>